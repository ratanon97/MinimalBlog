<%- include("partials/header") -%>

<h1><%= composeTitle %></h1>
<form action="/compose" method="post">
    <div class="form-group">
        <label for="newPostTitle">Title</label>
        <input type="text" class="form-control" name="postTitle" autocomplete="off" cols="100" rows="10">
    </div>
    <div class ="form-group">
        <label for="">Post</label>
        <textarea name="postText" class="form-control" cols="100" rows="10"></textarea>
    </div>
    <button type="submit" class="btn btn-primary" name="button">Publish</button>
</form>

<%- include("partials/footer") -%>