<%- include("partials/header") -%>

<h1><%= composeTitle %></h1>
<form action="/compose" method="post">
    <label>Title</label>
    <input type="text" name="postTitle" autocomplete="off" cols="100" rows="10">
    <textarea name="postTitle" cols="100" rows="10"></textarea>
    <label>Post</label>
    <textarea name="postText" cols="100" rows="10"></textarea>
    <button type="submit" name="button">Publish</button>
</form>

<%- include("partials/footer") -%>